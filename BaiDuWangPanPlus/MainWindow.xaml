<Window x:Class="BaiDuWangPanPlus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:BaiDuWangPanPlus"
        mc:Ignorable="d"
        Title="Star-百度网盘自动同步插件 V1.0.0" Height="450" Width="800" WindowStartupLocation="CenterScreen">

    <DockPanel>
        <StackPanel>
            <!--用户信息-->
            <materialDesign:Chip Content="{Binding baidu_name}">
                <materialDesign:Chip.Icon>
                    <Image Source="{Binding avatar_url}" />
                </materialDesign:Chip.Icon>
            </materialDesign:Chip>
            
        </StackPanel>
    </DockPanel>
</Window>
